{
   "manifest_version":2,
   "name":"Do Not Discard",
   "description":"Avoid Chrome automatically discarding tabs that it thinks don't interest to you when they do.",
   "version":"1.0",
  "background": {
    "scripts": ["background.js"]
  },
   "browser_action":{
      "default_popup":"popup.html",
      "default_icon":"icon.png"
   },
   "icons":{
      "16":"icon.png",
      "48":"icon.png",
      "128":"icon.png"
   },
   "permissions":[
      "tabs"
    ]
 }
